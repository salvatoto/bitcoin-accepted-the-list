generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  directUrl         = env("DIRECT_URL")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model providers {
  id                 BigInt    @id @default(autoincrement())
  created_at         DateTime? @default(now()) @db.Timestamptz(6)
  location           String[]
  services           String[]
  phone              String?
  email              String?
  website            String?
  nostr              String?
  twitter            String?
  instagram          String?
  onchain_accepted   Boolean?
  lightning_accepted Boolean?
  description        String?
  profile_url        String?
  languages          String[]
  name               String?
}

model providers_intermediate {
  id                 BigInt    @id @default(autoincrement())
  created_at         DateTime? @default(now()) @db.Timestamptz(6)
  location           String[]
  services           String[]
  phone              String?
  email              String?
  website            String?
  nostr              String?
  twitter            String?
  instagram          String?
  onchain_accepted   Boolean?
  lightning_accepted Boolean?
  description        String?
  profile_url        String?
  languages          String[]
  name               String?
}
